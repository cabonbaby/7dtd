<configs>
    <!-- If you want to change the unread books icon, here is how to do it -->
    <!-- QQ群326029473 -->
	<!-- QQ群326029473 -->
    <!-- QQ群326029473 -->	
    <set xpath="/items/item[@name='schematicMaster']/property[@name='AltItemTypeIcon']/@value">check</set>
    <insertAfter xpath="/items/item[@name='schematicMaster']/property[@name='AltItemTypeIcon']">
        <property name="AltItemTypeIconColor" value="0,256,0,256" />
    </insertAfter>
</configs>